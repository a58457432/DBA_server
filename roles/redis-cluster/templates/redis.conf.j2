daemonize       yes
pidfile         /var/run/redis.pid
port    6379
timeout 0
tcp-keepalive   60
tcp-backlog 511
loglevel        notice
logfile /var/log/redis.log
databases       16
dbfilename      redis.rdb
stop-writes-on-bgsave-error yes
rdbcompression  yes
rdbchecksum     yes
dir            /data/redis
slave-serve-stale-data  yes
slave-read-only yes
repl-diskless-sync no
repl-backlog-size 256MB
repl-backlog-ttl 7200
repl-disable-tcp-nodelay        no
repl-ping-slave-period  30    
repl-timeout 300  
slave-priority  100
maxclients      50000
maxmemory       7GB
maxmemory-policy allkeys-lru
maxmemory-samples 5
appendonly      no
appendfilename  redis.aof
appendfsync     everysec
no-appendfsync-on-rewrite       yes
auto-aof-rewrite-percentage     100
auto-aof-rewrite-min-size       1000GB
aof-load-truncated      yes
lua-time-limit  5000
slowlog-log-slower-than 10000
slowlog-max-len 128
activerehashing yes
hash-max-ziplist-entries        1000
hash-max-ziplist-value  128
list-max-ziplist-entries        1000
list-max-ziplist-value  128
set-max-intset-entries  1000
zset-max-ziplist-entries        1000
zset-max-ziplist-value  128
hll-sparse-max-bytes 3000
cluster-enabled yes
cluster-config-file nodes.conf
cluster-node-timeout 15000
cluster-slave-validity-factor 10
cluster-migration-barrier 1
cluster-require-full-coverage no
client-output-buffer-limit      normal 0 0 0
client-output-buffer-limit      slave 256000mb 64000mb 60
client-output-buffer-limit      pubsub 32mb 8mb 60
hz      10
aof-rewrite-incremental-fsync   yes
rename-command KEYS "HUPUKEYS"
rename-command FLUSHALL ""
rename-command FLUSHDB ""
rename-command SHUTDOWN "HUPUSHUTDOWN"                    
